<div id='sound_notify' style=''></div>

<script language='javascript' type='text/javascript'>

function sound_notify(){
  document.getElementById('sound_notify').innerHTML="<object height='1' width='1' id='audioplayer2' data='/mediaplayer/audioplayer.swf' type='application/x-shockwave-flash'><param value='audio/player.swf' name='movie'/><param value='playerID=2&amp;autostart=yes&amp;bg=0x444444&amp;leftbg=0x000000&amp;lefticon=0xFFFFFF&amp;rightbg=0x000000&amp;rightbghover=0x222222&amp;righticon=0xFFFFFF&amp;righticonhover=0x0080FF&amp;text=0xFFFFFF&amp;slider=0x0080FF&amp;track=0xFFFFFF&amp;border=0x0080FF&amp;loader=0x000000&amp;soundFile=/mediaplayer/ding.mp3' name='FlashVars'/><param value='high' name='quality'/><param value='true' name='menu'/><param value='transparent' name='wmode'/></object>";
}

var msg_return = '';

var $m = jQuery.noConflict();
var messageDivObject = $m('#messagediv');

function checkObject(object, checker) {
  var new_return = '';
	$m.get('/infusions/user_info_panel/' + checker, function(data) {
    new_return = data;
    object.html(new_return);
	});
	
  
  /*
   * For messages, a ring will sound if there is a new message.
   */
  if(checker == 'messagecheck.php'){
    if (msg_return == '') {
      msg_return = new_return;
    }

    
    if (msg_return != new_return) {
      sound_notify();
      msg_return = new_return;
    }
  }
  
  setTimeout(function(){
    checkObject(object, checker);
  }, 10000);
}

checkObject(messageDivObject, 'message_check.php');

function fadeBeginObject(object) {
   object.fadeOut(0, function () {
      fadeInObject(object);
   });
}

fadeBeginObject(messageDivObject);

function fadeOutObject(object) {
   object.fadeOut(1500, function () {
      fadeInObject(object);
   });
}

function fadeInObject(object) {
   object.fadeIn(1500, function () {
    setTimeout(function(){
      fadeOutObject(object);
    },3000);
   });
}

</script>


