<script language='javascript' type='text/javascript'>

function checkOnlineChat() {

  var $chato = jQuery.noConflict();
  var chatObject = $chato('#chatonline');

  $chato.get('/chat/onlinecheck.php', function(data) {
    if (chatObject.html().replace(/(<([^>]+)>)/ig,'') != data.replace(/(<([^>]+)>)/ig,'')) {
      chatObject.fadeOut(200, function () {
      chatObject.html(data).fadeIn(200);
      });
    }
  });
  
  setTimeout('checkOnlineChat()', 15000);
}

checkOnlineChat();

</script>

