<script language='javascript' type='text/javascript'>
/*
 * Written by: Qwexotic
 * Edited by: Null Set (08 Aug 2012)
 */
function checkOnline() {

  /*
   * We cache the object so as to save on "selecting" resources. Since this
   * code will be run multiple times, it will be well worth the little extra
   * boost in efficiency. - Null Set
   */
  var currentObject = jQuery('#onlinediv');
  jQuery.get('/infusions/online_users_panel/onlinecheck.php',function(data) {
    if (currentObject.html().replace(/(<([^>]+)>)/ig,'') != data.replace(/(<([^>]+)>)/ig,'')) {
        currentObject.fadeOut(1000, function () {
          currentObject.html(data);
          currentObject.fadeIn(1000);
        });
      }
  });
  setTimeout('checkOnline()', 10000);
}

checkOnline();

</script>


