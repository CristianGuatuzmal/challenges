<script type='text/javascript' src='/chat/js/chat.js'></script>
<script type='text/javascript'>
var $chat = jQuery.noConflict();$chat(document).ready(function(){
	//Adjust panel height
	$chat.fn.adjustPanel = function(){
		$chat(this).find('ul, .subpanel').css({ 'height' : 'auto'}); //Reset subpanel and ul height
		var windowHeight = $chat(window).height(); //Get the height of the browser viewport
		var panelsub = $chat(this).find('.subpanel').height(); //Get the height of subpanel
		var panelAdjust = windowHeight - 100; //Viewport height - 100px (Sets max height of subpanel)
		var ulAdjust =  panelAdjust - 25; //Calculate ul size after adjusting sub-panel (27px is the height of the base panel)
		if ( panelsub >= panelAdjust ) {	 //If subpanel is taller than max height...
			$chat(this).find('.subpanel').css({ 'height' : panelAdjust }); //Adjust subpanel to max height
			$chat(this).find('ul').css({ 'height' : ulAdjust}); //Adjust subpanel ul to new size
		}
		else if ( panelsub < panelAdjust ) { //If subpanel is smaller than max height...
			$chat(this).find('ul').css({ 'height' : 'auto'}); //Set subpanel ul to auto (default size)
		}
	};
	//Execute function on load
	$chat('#chatpanel').adjustPanel(); //Run the adjustPanel function on #chatpanel
	$chat('#alertpanel').adjustPanel(); //Run the adjustPanel function on #alertpanel
	//Each time the viewport is adjusted/resized, execute the function
	$chat(window).resize(function () {
		$chat('#chatpanel').adjustPanel();
		$chat('#alertpanel').adjustPanel();
	});
	//Click event on Chat Panel + Alert Panel
	$chat('#chatpanel a:first, #alertpanel a:first').click(function() { //If clicked on the first link of #chatpanel and #alertpanel...
		if($chat(this).next('.subpanel').is(':visible')){ //If subpanel is already active...
			$chat(this).next('.subpanel').hide(); //Hide active subpanel
			$chat('#footpanel li a').removeClass('active'); //Remove active class on the subpanel trigger
		}
		else { //if subpanel is not active...
			$chat('.subpanel').hide(); //Hide all subpanels
			$chat(this).next('.subpanel').toggle(); //Toggle the subpanel to make active
			$chat('#footpanel li a').removeClass('active'); //Remove active class on all subpanel trigger
			$chat(this).toggleClass('active'); //Toggle the active class on the subpanel trigger
		}
		return false; //Prevent browser jump to link anchor
	});
	//Click event outside of subpanel
	$chat(document).click(function() { //Click anywhere and...
		$chat('.subpanel').hide(); //hide subpanel
		$chat('#footpanel li a').removeClass('active'); //remove active class on subpanel trigger
	});
	$chat('.subpanel ul').click(function(e) {
		e.stopPropagation(); //Prevents the subpanel ul from closing on click
	});
	//Delete icons on Alert Panel
	$chat('#alertpanel li').hover(function() {
		$chat(this).find('a.delete').css({'visibility': 'visible'}); //Show delete icon on hover
	},function() {
		$chat(this).find('a.delete').css({'visibility': 'hidden'}); //Hide delete icon on hover out
	});
});
</script>
